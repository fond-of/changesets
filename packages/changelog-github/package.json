  "description": "A changelog entry generator for GitHub that links to commits, PRs and users",
  "main": "dist/fond-of-changesets-changelog-github.cjs.js",
  "module": "dist/fond-of-changesets-changelog-github.esm.js",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/fond-of-changesets-changelog-github.cjs.mjs",
        "default": "./dist/fond-of-changesets-changelog-github.cjs.js"
      },
      "module": "./dist/fond-of-changesets-changelog-github.esm.js",
      "import": "./dist/fond-of-changesets-changelog-github.cjs.mjs",
      "default": "./dist/fond-of-changesets-changelog-github.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "license": "MIT",
  "repository": "https://github.com/changesets/changesets/tree/main/packages/changelog-github",
  "dependencies": {
    "@fond-of/changesets-get-github-info": "workspace:^",
    "@fond-of/changesets-types": "workspace:^",
    "dotenv": "^8.1.0"
  },
